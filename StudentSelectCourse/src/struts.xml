<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="index" extends="struts-default">
		<action name="User_*" class="action.UserAction" method="{1}">
			<result name="3success">/main/MyJsp.jsp</result>
			<result name="2success">/main/MyJsp.jsp</result>
			<result name="1success">/main/MyJsp.jsp</result>
			<result name="loginfail">/index.jsp</result>
			<result name="Resuccess">/index.jsp</result>
			<result name="Refail">/register.jsp</result>
			<result name="fensuccess">/user/userall.jsp</result>
			<result name="pwdsuccess">/user/pwdupdate.jsp</result>
			<result name="pwdfail">/user/pwdupdate.jsp</result>
			<result name="desuccess">/user/userall.jsp</result>
			<result name="dedfail">/user/userall.jsp</result>
			<result name="addsuccess">/user/useradd.jsp</result>
			<result name="adddfail">/user/useradd.jsp</result>
			<result name="uusuccess">/user/userUpadte.jsp</result>
			<result name="uufail">/user/userUpadte.jsp</result>
			<result name="findsuccess">/user/userone.jsp</result>
			<result name="findfail">/user/userno.jsp</result>
			<result name="findnamesuccess">/user/userone.jsp</result>
			<result name="findnamefail">/user/userno.jsp</result>
			<result name="findrolesuccess">/user/userone.jsp</result>
			<result name="findrolefail">/user/userno.jsp</result>
			<result name="noadminsuccess">/user/noadmin.jsp</result>
			<result name="noadminfail">/user/noadmin.jsp</result>
		</action>
		<action name="Course_*" class="action.CourseAction" method="{1}">
			<result name="coursesuccess">/course/courseall.jsp</result>
			<result name="defail">/course/courseall.jsp</result>
			<result name="courseaddqsuccess">/course/courseAdd.jsp</result>
			<result name="addfail">/course/courseAdd.jsp</result>
			<result name="idsuccess">/course/courseone.jsp</result>
			<result name="idfail">/course/courseoneno.jsp</result>
			<result name="typesuccess">/course/courseone.jsp</result>
			<result name="typefail">/course/courseoneno.jsp</result>
			<result name="upqsuccess">/course/update.jsp</result>
			<result name="Resuccess">/course/update.jsp</result>
			<result name="Repefail">/course/update.jsp</result>
		</action>
		<action name="Tea_*" class="action.TeaAction" method="{1}">
			<result name="allsuccess">/teacher/teaall.jsp</result>
			<result name="addfail">/teacher/teaAdd.jsp</result>
			<result name="onesuccess">/teacher/teaone.jsp</result>
			<result name="onefail">/teacher/teano.jsp</result>
		</action>
		<action name="Stu_*" class="action.StuAction" method="{1}">
			<result name="allsuccess">/student/stuall.jsp</result>
			<result name="onesuccess">/student/stuone.jsp</result>
			<result name="onefail">/student/stuno.jsp</result>
			<result name="defail">/student/stuall.jsp</result>
			<result name="addfail">/student/stuAdd.jsp</result>
			<result name="update1">/student/stupdate.jsp</result>
			<result name="upfail">/student/stupdate.jsp</result>
		</action>
		<action name="Stucou_*" class="action.StucouAction" method="{1}">
			<result name="success">/stucou/stucou.jsp</result>
			<result name="fail">/stucou/stucou.jsp</result>
			<result name="allsuccess">/stucou/stuall.jsp</result>
			<result name="stucou">/stucou/couone.jsp</result>
			<result name="xsuccess">/stucou/couone.jsp</result>
			<result name="xfail">/stucou/couone.jsp</result>
			<result name="alsuccess">/stucou/already.jsp</result>
			<result name="alfail">/stucou/already.jsp</result>

		</action>
	</package>
</struts>
